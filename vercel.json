{
      "version": 2,
      "builds": [
        {
          "src": "Backend/server.js",
          "use": "@vercel/node"
        },
        {
          "src": "Frontend/vite.config.js",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "Frontend/dist"
          }
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/api/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/Frontend/dist/index.html"
        }
      ]
    }
    